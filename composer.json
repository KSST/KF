{
    "name": "ksst/kf",
    "description": "Koch Framework",
    "homepage": "http://clansuite.com/",
    "license": "GPL-2.0+",
    "keywords": ["framework", "full-stack", "cms", "cmf"],
    "type": "library",
    "authors": [
        {
            "name": "Jens-AndrÃ© Koch",
            "email": "jakoch@web.de"
        }
    ],
    "support": {
        "irc": "irc://irc.freenode.org/koch",
        "source": "https://github.com/KSST/KF/",
        "issues": "https://github.com/KSST/KF/issues/"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "php-gettext/php-gettext",
                "version": "1.0.11",
                "dist": {
                    "type": "tar",
                    "url": "http://launchpadlibrarian.net/61225817/php-gettext-1.0.11.tar.gz"
                },
                "autoload": {
                    "classmap": [
                        "gettext.php",
                        "streams.php"
                    ]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.3.3",

        "doctrine/orm"       : "2.3.0",
        "doctrine/dbal"      : "2.3.0",
        "doctrine/common"    : "2.3.0",
        "doctrine/migrations": "dev-master",
        "gedmo/doctrine-extensions": "dev-master",

        "smarty/smarty": "v3.1.12",
        "simplepie/simplepie": "dev-master",
        "swiftmailer/swiftmailer": "v4.2.1",
        "firephp/firephp-core": "dev-master",
        "php-gettext/php-gettext" : "v1.0.11"
    },
    "require-dev": {
        "mikey179/vfsStream": "*",
        "phpunit/PHPUnit": "3.7.*"
    },
    "suggest": {
        "ext-apc" : "*",
        "ext-gettext" : "*",
        "ext-intl" : "*",
        "ext-json" : "*",
        "ext-mbstring" : "*",
        "ext-yaml" : "*"
    },
    "autoload": {
        "psr-0": {
            "Koch\\": "framework/",
            "KochTest\\": "tests/"
        }
    },
    "minimum-stability": "dev"
}